<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br><br>
  <!-- <%= f.collection_select :artist, Artist.all, :id, :name %><br> -->
  <%= f.label "Artist" %>
  <%= artist_select @artist, @song %><br>
  <!-- <%= f.label :artist_name %>
  <%= f.text_field :artist_name %> -->
  <%= f.hidden_field :artist_id unless @artist.nil?%><br>
  <br />

  <%= f.submit %>
<% end %>
